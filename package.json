{
  "name": "mc_server_bot_ds",
  "version": "1.0.0",
  "description": "DS Bot to interact with an MC Server via Pterodactyl API",
  "main": "build/index.js",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "dev": "nodemon src/index.ts",
    "start:dev": "npx nodemon",
    "start": "yarn build && node build/index.js",
    "prodStart": "$PWD/run.sh",
    "prodBuild": "$PWD/build.sh",
    "refresh:slash": "yarn build && node build/scripts/registerSlashes.js"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/node": "^18.11.11",
    "@types/node-cron": "^3.0.6",
    "@types/ssh2-sftp-client": "^9.0.0",
    "archiver": "^5.3.1",
    "dotenv": "^16.0.3",
    "nodemon": "^2.0.20",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.238.0",
    "axios": "^1.2.1",
    "discord.js": "^14.7.1",
    "node-cron": "^3.0.2",
    "ssh2-sftp-client": "^9.0.4"
  }
}
